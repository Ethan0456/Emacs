#include <bits/stdc++.h>
using namespace std;

int main(){
  int test;
  cin >> test;
  while (test--){
    int a,b,c,d;
    cin >> a >> b >> c >> d;
    int temp1 = a*d;
    int temp2 = b*c;
    if (temp1 == temp2) cout << 0 << endl;
    else if (temp1 == 0 || temp2 == 0) cout << 1 << endl;
    else if (temp1 % temp2 == 0 || temp2 % temp1 == 0) cout << 2 << endl;
  }
  return 0;
}
